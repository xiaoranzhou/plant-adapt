  "database_schema": {
    "core_tables": {
      "studies": {
        "primary_key": "study_id",
        "fields": [
          "title",
          "species",
          "environmental_conditions",
          "institutes",
          "geographic_region"
        ],
        "description": "Main study information and experimental design"
      },
      "subjects": {
        "primary_key": "subject_id",
        "fields": [
          "genotype",
          "accession",
          "study_id",
          "treatment_conditions"
        ],
        "description": "Individual plant subjects and their genetic/treatment information"
      },
      "measurements": {
        "primary_key": "measurement_id",
        "fields": [
          "subject_id",
          "trait_name",
          "value",
          "unit",
          "measurement_date",
          "tissue_type"
        ],
        "description": "Individual trait measurements with metadata"
      },
      "environmental_conditions": {
        "primary_key": "condition_id",
        "fields": [
          "study_id",
          "temperature",
          "water_availability",
          "stress_type",
          "location"
        ],
        "description": "Environmental conditions for each study"
      },
      "traits": {
        "primary_key": "trait_id",
        "fields": [
          "trait_name",
          "category",
          "adaptation_relevance",
          "measurement_method"
        ],
        "description": "Trait definitions and categorizations"
      }
    },
    "relationships": {
      "studies_to_subjects": "one_to_many",
      "subjects_to_measurements": "one_to_many",
      "studies_to_conditions": "one_to_many",
      "measurements_to_traits": "many_to_one"
    }
  },
